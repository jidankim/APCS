public class ArrayIO{

    public static void printArray(Object[] x){
	for (int i = 0; i < x.length; i++)
	    System.out.print(x[i] + " ");
       System.out.println();
    }

    public static void printArray(Object[][] x){
	for (int i = 0; i < x.length; i++){
	    for (int j = 0; j < x[i].length; j++){
		System.out.print(x[i][j] + " ");
	    }
	    System.out.println();
	}
    }

}
